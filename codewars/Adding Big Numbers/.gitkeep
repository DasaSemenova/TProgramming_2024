function add(n, m) {
 while (n.length < m.length) {
 n = '0' + n;
    }
 while (m.length < n.length) {
 m = '0' + m;
    }
 let big_digit = 0;
 let result = '';

 for (let i = n.length - 1; i >= 0; i--) {
 const sum = parseInt(n[i]) + parseInt(m[i]) + big_digit;
 result = (sum % 10) + result; 
 big_digit = Math.floor(sum / 10); 
    }
 if (big_digit > 0) {
 result = big_digit + result;
    }
 return result;
}